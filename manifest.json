{
	"name": "Tab Killer",
	"version": "1.0.2",
	"description": "Kills given tab after idle timeout",
	"manifest_version": 3,
	"author": "Tristan Dobrowolski",
	"minimum_chrome_version": "92",
	"permissions": [
		"idle",
		"tabs",
		"storage"
	],
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	}
}